{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "DaftPlug Pro Plugins",
  "colors": {
    "primary": "#155dfc",
    "light": "#dbeafe",
    "dark": "#155dfc"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "icon": "book-open-cover",
          "href": "https://mintlify.com/docs"
        },
        {
          "anchor": "Blog",
          "icon": "newspaper",
          "href": "https://mintlify.com/blog"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Progressify",
        "icon": "images/progressify.png",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "Generatify",
        "icon": "images/generatify.png",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/images/logo.png",
    "dark": "/images/logo.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@daftplug.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Main Site",
      "href": "https://daftplug.com/"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/daftplug",
      "github": "https://github.com/daftplug",
      "linkedin": "https://linkedin.com/company/daftplug"
    }
  }
}
